<%==panel_helper "登陆" do%>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name),html:{role:"form",style:"width:300px;"}) do |f| %>
        <div class="form-group">
            <%= f.label :email %>
            <%= f.email_field :email, autofocus: true,class:"form-control" %>
        </div>
        <div class="form-group">
            <%= f.label :password %>
            <%= f.password_field :password, autocomplete: "off",class:"form-control" %>
        </div>
        <% if devise_mapping.rememberable? -%>
            <div class="form-group">
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me %>
            </div>
        <% end -%>
        <div class="actions">
            <%= f.submit "登陆",class:"btn btn-default btn-sm" %>
        </div>
    <% end %>
    <br>
    <%= render "devise/shared/links" %>
<%end%>

